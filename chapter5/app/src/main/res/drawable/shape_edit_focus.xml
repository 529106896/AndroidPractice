<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!--  形状内部填充颜色  -->
    <solid android:color="#ffffff"/>
    <!--  形状轮廓的粗细和颜色  -->
    <stroke android:width="1dp" android:color="#0000ff"/>
    <!--  形状四个角的半径  -->
    <corners android:radius="5dp"/>
    <padding
        android:bottom="2dp"
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"/>
</shape>